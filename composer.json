{
    "name": "nimbly/carton",
    "description": "A simple PSR-11 container implementation offering reflection based autowiring.",
    "type": "library",
    "require": {
        "php": "^8.2",
        "psr/container": "^1.0|^2.0",
        "nimbly/resolve": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "vimeo/psalm": "^6.0",
        "symfony/var-dumper": "^4.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Brent Scheffler",
            "email": "brent@brentscheffler.com"
        }
    ],
	"provide": {
		"psr/container-implementation": "^2.0"
	},
    "autoload": {
        "psr-4": {
            "Nimbly\\Carton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nimbly\\Carton\\Tests\\": "tests/"
		}
	}
}
